<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_navigation">

    <fragment
        android:id="@+id/mainFragment"
        android:name="com.example.heysrealprojcet.ui.main.MainFragment"
        android:label="MainFragment"
        tools:layout="@layout/main_fragment">

        <action
            android:id="@+id/action_mainFragment_to_joinPopupFragment"
            app:destination="@id/joinPopupFragment" />
        <action
            android:id="@+id/action_mainFragment_to_studyFragment"
            app:destination="@id/studyFragment" />
        <action
            android:id="@+id/action_mainFragment_to_joinPhoneFragment"
            app:destination="@id/joinPhoneFragment" />
        <action
            android:id="@+id/action_mainFragment_to_contestFragment"
            app:destination="@id/contestListFragment" />
        <action
            android:id="@+id/action_mainFragment_to_activityListFragment"
            app:destination="@id/activityListFragment" />
    </fragment>
    <fragment
        android:id="@+id/joinPopupFragment"
        android:name="com.example.heysrealprojcet.ui.join.JoinPopupFragment"
        android:label="JoinPopupFragment"
        tools:layout="@layout/join_popup_fragment">
        <action
            android:id="@+id/action_joinPopupFragment_to_joinPhoneFragment"
            app:destination="@id/joinPhoneFragment" />
    </fragment>
    <fragment
        android:id="@+id/joinPhoneFragment"
        android:name="com.example.heysrealprojcet.ui.join.phone.JoinPhoneFragment"
        android:label="JoinPhoneFragment"
        tools:layout="@layout/join_phone_fragment">
        <action
            android:id="@+id/action_joinPhoneFragment_to_phoneVerificationFragment"
            app:destination="@id/phoneVerificationFragment" />
    </fragment>
    <fragment
        android:id="@+id/joinPasswordFragment"
        android:name="com.example.heysrealprojcet.ui.join.password.JoinPasswordFragment"
        android:label="JoinPasswordFragment"
        tools:layout="@layout/join_password_fragment">
        <action
            android:id="@+id/action_joinPasswordFragment_to_joinNameFragment"
            app:destination="@id/joinNameFragment" />
        <action
            android:id="@+id/action_joinPasswordFragment_to_mainFragment"
            app:destination="@id/mainFragment" />
    </fragment>
    <fragment
        android:id="@+id/joinNameFragment"
        android:name="com.example.heysrealprojcet.ui.join.name.JoinNameFragment"
        android:label="JoinNameFragment"
        tools:layout="@layout/join_name_fragment">
        <action
            android:id="@+id/action_joinNameFragment_to_joinGenderFragment"
            app:destination="@id/joinGenderFragment" />
    </fragment>
    <fragment
        android:id="@+id/joinGenderFragment"
        android:name="com.example.heysrealprojcet.ui.join.gender.JoinGenderFragment"
        android:label="JoinGenderFragment"
        tools:layout="@layout/join_gender_fragment">
        <action
            android:id="@+id/action_joinGenderFragment_to_joinAgeFragment"
            app:destination="@id/joinAgeFragment" />
    </fragment>
    <fragment
        android:id="@+id/joinAgeFragment"
        android:name="com.example.heysrealprojcet.ui.join.age.JoinAgeFragment"
        android:label="JoinAgeFragment"
        tools:layout="@layout/join_age_fragment">
        <action
            android:id="@+id/action_joinAgeFragment_to_joinInterestFragment"
            app:destination="@id/joinInterestFragment" />
    </fragment>
    <fragment
        android:id="@+id/phoneVerificationFragment"
        android:name="com.example.heysrealprojcet.ui.join.phone.JoinVerificationFragment"
        android:label="PhoneVerificationFragment"
        tools:layout="@layout/join_phone_verification_fragment">
        <action
            android:id="@+id/action_phoneVerificationFragment_to_joinPasswordFragment"
            app:destination="@id/joinPasswordFragment" />
        <argument
            android:name="phoneNumber"
            android:defaultValue="010-1234-5678"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/joinInterestFragment"
        android:name="com.example.heysrealprojcet.ui.join.interest.JoinInterestFragment"
        android:label="JoinInterestFragment"
        tools:layout="@layout/join_interest_fragment">
        <action
            android:id="@+id/action_joinInterestFragment_to_mainFragment"
            app:destination="@id/mainFragment" />
    </fragment>
    <fragment
        android:id="@+id/studyFragment"
        android:name="com.example.heysrealprojcet.ui.main.content.study.StudyListFragment"
        android:label="study_fragment"
        tools:layout="@layout/study_list_fragment">
        <action
            android:id="@+id/action_studyFragment_to_studyFilterFragment"
            app:destination="@id/studyFilterFragment" />
    </fragment>
    <fragment
        android:id="@+id/contestActivityFilterFragment"
        android:name="com.example.heysrealprojcet.ui.main.content.contestActivity.filter.ContestActivityFilterFragment"
        android:label="FilterFragment"
        tools:layout="@layout/contest_activity_filter_fragment"></fragment>
    <fragment
        android:id="@+id/contestListFragment"
        android:name="com.example.heysrealprojcet.ui.main.content.contestActivity.contest.ContestListFragment"
        android:label="ContestFragment"
        tools:layout="@layout/contest_list_fragment">
        <action
            android:id="@+id/action_contestListFragment_to_contestActivityFilterFragment"
            app:destination="@id/contestActivityFilterFragment" />
        <action
            android:id="@+id/action_contestListFragment_to_contestActivityDetailFragment"
            app:destination="@id/contestActivityDetailFragment" />
    </fragment>
    <fragment
        android:id="@+id/contestActivityDetailFragment"
        android:name="com.example.heysrealprojcet.ui.main.content.contestActivity.detail.ContestActivityDetailFragment"
        android:label="ContestActivityDetailFragment"
        tools:layout="@layout/contest_activity_detail_fragment">
        <action
            android:id="@+id/action_contestActivityDetailFragment_to_heysChannelListFragment"
            app:destination="@id/heysChannelListFragment" />
    </fragment>
    <fragment
        android:id="@+id/heysChannelListFragment"
        android:name="com.example.heysrealprojcet.ui.main.content.channel.HeysChannelListFragment"
        android:label="HeysListFragment"
        tools:layout="@layout/heys_channel_list_fragment">
        <action
            android:id="@+id/action_heysChannelListFragment_to_heysChannelDetailFragment"
            app:destination="@id/heysChannelDetailFragment" />
    </fragment>
    <fragment
        android:id="@+id/studyFilterFragment"
        android:name="com.example.heysrealprojcet.ui.main.content.study.filter.StudyFilterFragment"
        android:label="StudyFilterFragment"
        tools:layout="@layout/study_filter_fragment" />
    <fragment
        android:id="@+id/heysChannelDetailFragment"
        android:name="com.example.heysrealprojcet.ui.main.content.channel.HeysChannelDetailFragment"
        android:label="HeysChannelDetailFragment"
        tools:layout="@layout/heys_channel_detail_fragment" >
        <action
            android:id="@+id/action_heysChannelDetailFragment_to_memberHeysListFragment"
            app:destination="@id/memberHeysListFragment" />
    </fragment>
    <fragment
        android:id="@+id/activityListFragment"
        android:name="com.example.heysrealprojcet.ui.main.content.contestActivity.activity.ActivityListFragment"
        android:label="ActivityListFragment"
        tools:layout="@layout/activity_list_fragment">
        <action
            android:id="@+id/action_activityListFragment_to_contestActivityFilterFragment"
            app:destination="@id/contestActivityFilterFragment" />
        <action
            android:id="@+id/action_activityListFragment_to_contestActivityDetailFragment"
            app:destination="@id/contestActivityDetailFragment" />
    </fragment>
    <fragment
        tools:layout="@layout/member_heys_list_fragment"
        android:id="@+id/memberHeysListFragment"
        android:name="com.example.heysrealprojcet.ui.main.content.channel.MemberHeysListFragment"
        android:label="MemberHeysListFragment" />
</navigation>
