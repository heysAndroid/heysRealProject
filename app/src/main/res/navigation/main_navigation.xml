<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_navigation"
    app:startDestination="@id/mainFragment">

    <fragment
        android:id="@+id/mainFragment"
        android:name="com.example.heysrealprojcet.ui.main.MainFragment"
        android:label="MainFragment"
        tools:layout="@layout/main_fragment">

        <action
            android:id="@+id/action_mainFragment_to_studyFragment"
            app:destination="@id/studyFragment" />
        <action
            android:id="@+id/action_mainFragment_to_contestFragment"
            app:destination="@id/contestListFragment" />
        <action
            android:id="@+id/action_mainFragment_to_extracurricularListFragment"
            app:destination="@id/extracurricularListFragment" />
        <action
            android:id="@+id/action_mainFragment_to_channelNameFragment"
            app:destination="@id/channelNameFragment" />
        <argument
            android:name="isNewUser"
            android:defaultValue="false"
            app:argType="boolean" />
    </fragment>
    <fragment
        android:id="@+id/studyFragment"
        android:name="com.example.heysrealprojcet.ui.main.content.study.StudyListFragment"
        android:label="study_fragment"
        tools:layout="@layout/study_list_fragment">
        <action
            android:id="@+id/action_studyFragment_to_studyFilterFragment"
            app:destination="@id/studyFilterFragment" />
        <action
            android:id="@+id/action_studyFragment_to_heysChannelDetailFragment"
            app:destination="@id/channelDetailFragment" />
    </fragment>
    <fragment
        android:id="@+id/contestExtracurricularFilterFragment"
        android:name="com.example.heysrealprojcet.ui.main.content.contestExtracurricular.filter.ContestExtracurricularFilterFragment"
        android:label="FilterFragment"
        tools:layout="@layout/contest_extracurricular_filter_fragment"></fragment>
    <fragment
        android:id="@+id/contestListFragment"
        android:name="com.example.heysrealprojcet.ui.main.content.contestExtracurricular.contest.ContestListFragment"
        android:label="ContestFragment"
        tools:layout="@layout/contest_list_fragment">
        <action
            android:id="@+id/action_contestListFragment_to_contestExtracurricularFilterFragment"
            app:destination="@id/contestExtracurricularFilterFragment" />
        <action
            android:id="@+id/action_contestListFragment_to_contestExtracurricularDetailFragment"
            app:destination="@id/contestExtracurricularDetailFragment" />
    </fragment>
    <fragment
        android:id="@+id/contestExtracurricularDetailFragment"
        android:name="com.example.heysrealprojcet.ui.main.content.contestExtracurricular.contest.detail.ContestExtracurricularDetailFragment"
        android:label="ContestExtracurricularDetailFragment"
        tools:layout="@layout/contest_extracurricular_detail_fragment">
        <action
            android:id="@+id/action_contestExtracurricularDetailFragment_to_channelListFragment"
            app:destination="@id/channelListFragment" />
        <action
            android:id="@+id/action_contestExtracurricularDetailFragment_to_contestDetailLookFragment"
            app:destination="@id/ImageZoomFragment" />
    </fragment>
    <fragment
        android:id="@+id/channelListFragment"
        android:name="com.example.heysrealprojcet.ui.channel.list.ChannelListFragment"
        android:label="ChannelListFragment"
        tools:layout="@layout/channel_list_fragment">
        <action
            android:id="@+id/action_channelListFragment_to_channelDetailFragment"
            app:destination="@id/channelDetailFragment" />
        <action
            android:id="@+id/action_channelListFragment_to_channelNameFragment"
            app:destination="@id/channelNameFragment" />
    </fragment>
    <fragment
        android:id="@+id/studyFilterFragment"
        android:name="com.example.heysrealprojcet.ui.main.content.study.filter.StudyFilterFragment"
        android:label="StudyFilterFragment"
        tools:layout="@layout/study_filter_fragment" />
    <fragment
        android:id="@+id/channelDetailFragment"
        android:name="com.example.heysrealprojcet.ui.channel.list.ChannelDetailFragment"
        android:label="ChannelDetailFragment"
        tools:layout="@layout/channel_detail_fragment">
        <action
            android:id="@+id/action_channelDetailFragment_to_memberHeysListFragment"
            app:destination="@id/memberHeysListFragment" />
        <action
            android:id="@+id/action_channelDetailFragment_to_waitingHeyListFragment"
            app:destination="@id/waitingHeyListFragment" />
        <action
            android:id="@+id/action_channelDetailFragment_to_myChannelFragment"
            app:destination="@id/myChannelFragment" />
    </fragment>
    <fragment
        android:id="@+id/extracurricularListFragment"
        android:name="com.example.heysrealprojcet.ui.main.content.contestExtracurricular.extracurricular.ExtracurricularListFragment"
        android:label="ExtracurricularListFragment"
        tools:layout="@layout/extracurricular_list_fragment">
        <action
            android:id="@+id/action_extracurricularListFragment_to_contestExtracurricularFilterFragment"
            app:destination="@id/contestExtracurricularFilterFragment" />
        <action
            android:id="@+id/action_extracurricularListFragment_to_contestExtracurricularDetailFragment"
            app:destination="@id/contestExtracurricularDetailFragment" />
    </fragment>
    <fragment
        android:id="@+id/memberHeysListFragment"
        android:name="com.example.heysrealprojcet.ui.main.content.study.hey.participating.MemberHeysListFragment"
        android:label="MemberHeysListFragment"
        tools:layout="@layout/member_heys_list_fragment">
        <action
            android:id="@+id/action_memberHeysListFragment_to_channelChatFragment"
            app:destination="@id/channelChatFragment" />
    </fragment>
    <fragment
        android:id="@+id/myChannelFragment"
        android:name="com.example.heysrealprojcet.ui.user.myChannel.MyChannelFragment"
        android:label="MyChannelFragment"
        tools:layout="@layout/my_channel_fragment">
        <action
            android:id="@+id/action_myChannelFragment_to_myChannelNotificationFragment"
            app:destination="@id/myChannelNotificationFragment" />
        <action
            android:id="@+id/action_myChannelFragment_to_myChannelPrivateNotificationFragment"
            app:destination="@id/myChannelPrivateNotificationFragment" />
        <action
            android:id="@+id/action_myChannelFragment_to_myChannelCattingReader"
            app:destination="@id/myChannelCattingReader" />
        <action
            android:id="@+id/action_myChannelFragment_to_chattingFragment"
            app:destination="@id/chattingFragment" />
    </fragment>
    <fragment
        android:id="@+id/myPageFragment"
        android:name="com.example.heysrealprojcet.ui.user.myPage.MyPageFragment"
        android:label="MyPageFragment"
        tools:layout="@layout/my_page_fragment">
        <action
            android:id="@+id/action_myPageFragment_to_settingFragment"
            app:destination="@id/settingFragment" />
        <action
            android:id="@+id/action_myPageFragment_to_engagedChannelListFragment"
            app:destination="@id/engagedChannelListFragment" />
        <action
            android:id="@+id/action_myPageFragment_to_waitingChannelListFragment"
            app:destination="@id/waitingChannelListFragment" />
        <action
            android:id="@+id/action_myPageFragment_to_profileEditFragment"
            app:destination="@id/profileEditFragment" />
    </fragment>
    <fragment
        android:id="@+id/channelNameFragment"
        android:name="com.example.heysrealprojcet.ui.channel.create.name.ChannelNameFragment"
        android:label="ChannelNameFragment"
        tools:layout="@layout/channel_name_fragment">
        <action
            android:id="@+id/action_channelNameFragment_to_channelInformFragment"
            app:destination="@id/channelInformFragment" />
    </fragment>
    <fragment
        android:id="@+id/channelInformFragment"
        android:name="com.example.heysrealprojcet.ui.channel.create.inform.ChannelInformFragment"
        android:label="ChannelInformFragment"
        tools:layout="@layout/channel_inform_fragment">
        <action
            android:id="@+id/action_channelInformFragment_to_channelFreeFragment"
            app:destination="@id/channelDescriptionFragment" />
    </fragment>
    <fragment
        android:id="@+id/channelDescriptionFragment"
        android:name="com.example.heysrealprojcet.ui.channel.create.description.ChannelDescriptionFragment"
        android:label="ChannelDescriptionFragment"
        tools:layout="@layout/channel_description_fragment">
        <action
            android:id="@+id/action_channelDescriptionFragment_to_channelPreviewFragment"
            app:destination="@id/channelPreviewFragment" />
    </fragment>
    <fragment
        android:id="@+id/channelPreviewFragment"
        android:name="com.example.heysrealprojcet.ui.channel.create.preview.ChannelPreviewFragment"
        android:label="ChannelPreviewFragment"
        tools:layout="@layout/channel_preview_fragment">
        <action
            android:id="@+id/action_channelPreviewFragment_to_channelDetailFragment"
            app:destination="@id/channelDetailFragment" />
        <action
            android:id="@+id/action_channelPreviewFragment_to_backButtonPopupFragment"
            app:destination="@id/backButtonPopupFragment" />
    </fragment>
    <fragment
        android:id="@+id/channelChatFragment"
        android:name="com.example.heysrealprojcet.ui.channel.chat.HeysChannelChatFragment"
        android:label="ChannelChatFragment"
        tools:layout="@layout/heys_channel_chat_fragment" />
    <fragment
        android:id="@+id/waitingHeyListFragment"
        android:name="com.example.heysrealprojcet.ui.main.content.study.hey.waiting.WaitingHeyListFragment"
        android:label="WaitingHeyListFragment"
        tools:layout="@layout/waiting_hey_list_fragment" />
    <fragment
        android:id="@+id/myChannelNotificationFragment"
        android:name="com.example.heysrealprojcet.ui.user.myChannel.notification.MyChannelNotificationFragment"
        android:label="MyChannelNotificationFragment"
        tools:layout="@layout/my_channel_notification_fragment" />
    <fragment
        android:id="@+id/myChannelPrivateNotificationFragment"
        android:name="com.example.heysrealprojcet.ui.user.myChannel.privateNotification.MyChannelPrivateNotificationFragment"
        android:label="MyChannelPrivateNotificationFragment"
        tools:layout="@layout/my_channel_private_notification_fragment" />
    <fragment
        android:id="@+id/myChannelCattingReader"
        android:name="com.example.heysrealprojcet.ui.user.myChannel.chat.MyChannelChattingReader"
        android:label="MyChannelCattingReader"
        tools:layout="@layout/my_channel_chatting_reader">
        <action
            android:id="@+id/action_myChannelCattingReader_to_chattingReaderNotice"
            app:destination="@id/chattingReaderNotice" />
        <action
            android:id="@+id/action_myChannelCattingReader_to_myChannelManageFragment"
            app:destination="@id/myChannelManageFragment" />
    </fragment>
    <fragment
        android:id="@+id/chattingReaderNotice"
        android:name="com.example.heysrealprojcet.ui.user.myChannel.chat.ChattingReaderNoticeFragment"
        android:label="ChattingReaderNotice"
        tools:layout="@layout/chatting_reader_notice">
        <action
            android:id="@+id/action_chattingReaderNotice_to_chattingReaderNoticePost"
            app:destination="@id/chattingReaderNoticePost" />
    </fragment>
    <fragment
        android:id="@+id/chattingReaderNoticePost"
        android:name="com.example.heysrealprojcet.ui.user.myChannel.chat.ChattingReaderNoticePost"
        android:label="ChattingReaderNoticePost"
        tools:layout="@layout/chatting_reader_notice_post" />
    <fragment
        android:id="@+id/myChannelManageFragment"
        android:name="com.example.heysrealprojcet.ui.user.myChannel.manage.MyChannelManageFragment"
        android:label="MyChannelManageFragment"
        tools:layout="@layout/my_channel_manage">
        <action
            android:id="@+id/action_myChannelManageFragment_to_waitingHeyListFragment"
            app:destination="@id/waitingHeyListFragment" />
        <action
            android:id="@+id/action_myChannelManageFragment_to_memberHeysListFragment"
            app:destination="@id/memberHeysListFragment" />
    </fragment>
    <fragment
        android:id="@+id/settingFragment"
        android:name="com.example.heysrealprojcet.ui.user.setting.SettingFragment"
        android:label="SettingFragment"
        tools:layout="@layout/setting_fragment">
        <action
            android:id="@+id/action_settingFragment_to_settingPhoneChangeFragment"
            app:destination="@id/settingPhoneChangeFragment" />
        <action
            android:id="@+id/action_settingFragment_to_settingPasswordVerificationFragment"
            app:destination="@id/settingPasswordVerificationFragment" />
        <action
            android:id="@+id/action_settingFragment_to_settingDeleteAccountFragment"
            app:destination="@id/settingDeleteAccountFragment" />
    </fragment>
    <fragment
        android:id="@+id/engagedChannelListFragment"
        android:name="com.example.heysrealprojcet.ui.user.channel.engagedChannel.EngagedChannelListFragment"
        android:label="EngagedChannelListFragment"
        tools:layout="@layout/engaged_channel_list_fragment" />
    <fragment
        android:id="@+id/settingPasswordChangeFragment"
        android:name="com.example.heysrealprojcet.ui.user.setting.password.SettingPasswordChangeFragment"
        android:label="SettingPasswordChangeFragment"
        tools:layout="@layout/setting_password_change_fragment">
        <action
            android:id="@+id/action_settingPasswordChangeFragment_to_settingFragment"
            app:destination="@id/settingFragment" />
    </fragment>
    <fragment
        android:id="@+id/settingPasswordForgetFragment"
        android:name="com.example.heysrealprojcet.ui.user.setting.password.SettingPasswordForgetFragment"
        android:label="SettingPasswordForgetFragment"
        tools:layout="@layout/setting_password_forget_fragment">
        <action
            android:id="@+id/action_settingPasswordForgetFragment_to_settingPasswordChangeFragment"
            app:destination="@id/settingPasswordChangeFragment" />
    </fragment>
    <fragment
        android:id="@+id/settingPasswordVerificationFragment"
        android:name="com.example.heysrealprojcet.ui.user.setting.password.SettingPasswordVerificationFragment"
        android:label="SettingPasswordVerificationFragment"
        tools:layout="@layout/setting_password_verification_fragment">
        <action
            android:id="@+id/action_settingPasswordVerificationFragment_to_settingPasswordChangeFragment"
            app:destination="@id/settingPasswordChangeFragment" />
        <action
            android:id="@+id/action_settingPasswordVerificationFragment_to_settingPasswordForgetFragment"
            app:destination="@id/settingPasswordForgetFragment" />
    </fragment>
    <fragment
        android:id="@+id/settingPhoneChangeFragment"
        android:name="com.example.heysrealprojcet.ui.user.setting.phone.SettingPhoneChangeFragment"
        android:label="SettingPhoneChangeFragment"
        tools:layout="@layout/setting_phone_change_fragment">
        <action
            android:id="@+id/action_settingPhoneChangeFragment_to_settingPhoneVerificationFragment"
            app:destination="@id/settingPhoneVerificationFragment" />
    </fragment>
    <fragment
        android:id="@+id/settingPhoneVerificationFragment"
        android:name="com.example.heysrealprojcet.ui.user.setting.phone.SettingPhoneVerificationFragment"
        android:label="SettingPhoneVerificationFragment"
        tools:layout="@layout/setting_phone_verification_fragment">
        <action
            android:id="@+id/action_settingPhoneVerificationFragment_to_settingFragment"
            app:destination="@id/settingFragment" />
    </fragment>
    <fragment
        android:id="@+id/waitingChannelListFragment"
        android:name="com.example.heysrealprojcet.ui.user.channel.waitingChannel.WaitingChannelListFragment"
        android:label="WaitingChannelListFragment"
        tools:layout="@layout/waiting_channel_list_fragment" />
    <fragment
        android:id="@+id/profileEditFragment"
        android:name="com.example.heysrealprojcet.ui.user.myPage.profile.ProfileEditFragment"
        android:label="ProfileEditFragment"
        tools:layout="@layout/profile_edit_fragment">
        <action
            android:id="@+id/action_profileEditFragment_to_myPageFragment"
            app:destination="@id/myPageFragment" />
    </fragment>
    <fragment
        android:id="@+id/settingDeleteAccountFragment"
        android:name="com.example.heysrealprojcet.ui.user.setting.delete.SettingDeleteAccountFragment"
        android:label="SettingDeleteAccountFragment"
        tools:layout="@layout/setting_delete_account_fragment">
        <action
            android:id="@+id/action_settingDeleteAccountFragment_to_settingDeleteDecideFragment"
            app:destination="@id/settingDeleteDecideFragment" />
    </fragment>
    <fragment
        android:id="@+id/settingDeleteDecideFragment"
        android:name="com.example.heysrealprojcet.ui.user.setting.delete.SettingDeleteDecideFragment"
        android:label="SettingDeleteDecideFragment"
        tools:layout="@layout/setting_delete_decide_fragment" />
    <fragment
        android:id="@+id/backButtonPopupFragment"
        android:name="com.example.heysrealprojcet.ui.channel.create.BackButtonPopupFragment"
        android:label="BackButtonPopupFragment"
        tools:layout="@layout/back_button_popup_fragment">
        <action
            android:id="@+id/action_backButtonPopupFragment_to_mainFragment"
            app:destination="@id/mainFragment"
            app:popUpTo="@id/mainFragment"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_backButtonPopupFragment_to_channelDetailFragment"
            app:destination="@id/channelDetailFragment" />
    </fragment>

    <fragment
        android:id="@+id/chattingFragment"
        android:name="com.example.heysrealprojcet.ui.user.myChannel.chat.ChattingFragment"
        android:label="ChattingFragment"
        tools:layout="@layout/chatting_fragment" />
    <fragment
        android:id="@+id/ImageZoomFragment"
        android:name="com.example.heysrealprojcet.ui.main.content.contestExtracurricular.contest.detail.ImageZoomFragment"
        android:label="ImageZoomFragment"
        tools:layout="@layout/image_zoom_fragment">
        <action
            android:id="@+id/action_imageZoomFragment_to_contestExtracurricularDetailFragment"
            app:destination="@id/contestExtracurricularDetailFragment" />
    </fragment>
</navigation>
